{
  "name": "nickel-scm",
  "version": "0.9.3",
  "description": "Handle tasks on multiple local Git repositories",
  "repository": {
    "type": "git",
    "url": "https://github.com/bredmold/nickel.git"
  },
  "scripts": {
    "build": "tsc && chmod a+x dist/src/nickel.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "format": "prettier --write '**/*.ts'"
  },
  "bin": {
    "nickel": "dist/src/nickel.js"
  },
  "keywords": [
    "git"
  ],
  "author": "Aaron Goldstein",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/tmp": "^0.1.0",
    "import-sort-style-eslint": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prettier-plugin-import-sort": "0.0.4",
    "tmp": "^0.1.0",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@types/node": "^13.1.2",
    "chalk": "^2.4.2",
    "commander": "^4.0.1",
    "winston": "^3.2.1"
  },
  "importSort": {
    ".ts": {
      "style": "eslint",
      "parser": "typescript"
    }
  }
}
